{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@altinget/vercel-builder",
      "config": {
        "images": {
          "sizes": [
            320,
            640,
            768,
            1024,
            1280,
            1536,
            1536
          ],
          "domains": ["media.valorant-api.com"],
          "minimumCacheTTL": 60,
          "formats": ["image/webp", "image/avif"]
        }
      }
    }
  ]
}
